<?php if (!empty($this->models)): ?>
    <ol class="bullets-none separated">
    	<?php foreach ($this->models as $modelDetails): ?>
    		<li>
    			<a class="Model <?php echo $modelDetails['selected'] ?>" href="<?php echo $modelDetails['url'] ?>" about="<?php echo $modelDetails['graphUri'] ?>">
